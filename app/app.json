{
  "expo": {
    "plugins": [
      [
        "expo-dev-client",
        {
          "launchMode": "most-recent"
        }
      ],
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "name": "Quiz Player",
    "slug": "quizPlayer",
    "version": "1.0.6",
    "orientation": "portrait",
    "icon": "./assets/logo-square-02.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "backgroundColor": "#1a1a1a",
    "splash": {
      "image": "./assets/logo-square-02.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a1a1a",
      "dark": {
        "image": "./assets/logo-square-02.png",
        "resizeMode": "contain",
        "backgroundColor": "#1a1a1a"
      }
    },
    "ios": {
      "supportsTablet": true,
      "backgroundColor": "#1a1a1a",
      "splash": {
        "image": "./assets/splash-logo-dark.png",
        "resizeMode": "contain",
        "backgroundColor": "#1a1a1a",
        "tabletImage": "./assets/splash-logo-dark.png",
        "dark": {
          "image": "./assets/splash-logo-dark.png",
          "resizeMode": "contain",
          "backgroundColor": "#1a1a1a",
          "tabletImage": "./assets/splash-logo-dark.png"
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-square-02.png",
        "backgroundColor": "#1a1a1a"
      },
      "icon": "./assets/logo-square-02.png",
      "backgroundColor": "#1a1a1a",
      "splash": {
        "image": "./assets/logo-square-02.png",
        "resizeMode": "contain",
        "backgroundColor": "#1a1a1a",
        "dark": {
          "image": "./assets/logo-square-02.png",
          "resizeMode": "contain",
          "backgroundColor": "#1a1a1a"
        }
      },
      "package": "com.andriiradchenko.quizPlayer",
      "versionCode": 12
    },
    "web": {
      "favicon": "./assets/logo-square-02.png",
      "backgroundColor": "#1a1a1a"
    },
    "extra": {
      "eas": {
        "projectId": "9fd7a24f-e33c-4210-b521-b73cb970ccab"
      }
    }
  }
}
